<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="202dp"
        android:layout_height="195dp"
        android:layout_gravity="center_horizontal|top">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="91dp"
            android:layout_height="50dp"
            android:layout_below="@+id/imageView1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="24dp"
            android:text="Menu"
            android:textSize="26sp"
            android:layout_gravity="center"
            android:backgroundTint="#000000"
            android:textColor="#010101" />

        <FrameLayout
            android:layout_width="144dp"
            android:layout_height="71dp"
            android:layout_gravity="center_horizontal|top">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Finalizar"
                android:id="@+id/buttonfinalizar"
                android:layout_gravity="center"
                android:onClick="onclick" />
        </FrameLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="121dp"
        android:layout_height="182dp"
        android:layout_gravity="left|center_vertical">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView7"
            android:src="@drawable/almoco"
            android:layout_gravity="right|bottom" />

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView6"
            android:src="@drawable/cafedamanha"
            android:layout_gravity="right|top" />

    </FrameLayout>

    <FrameLayout
        android:layout_width="146dp"
        android:layout_height="181dp"
        android:layout_gravity="left|bottom">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView8"
            android:src="@drawable/bebida"
            android:layout_gravity="center_horizontal|top" />

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/imageView9"
            android:src="@drawable/sobremesa"
            android:layout_gravity="center_horizontal|bottom" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="168dp"
        android:layout_height="126dp"
        android:layout_gravity="center">


        <Button
            android:id="@+id/buttonalmoco"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:text="  Refeição             "
            android:layout_gravity="right|bottom"
            android:onClick="onclick" />

        <Button
            android:id="@+id/buttoncafe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:text="Café da manhã "
            android:layout_gravity="right|top"
            android:onClick="onclick" />


    </FrameLayout>

    <FrameLayout
        android:layout_width="168dp"
        android:layout_height="156dp"
        android:layout_gravity="center_horizontal|bottom" >

        <Button
            android:id="@+id/buttonbebidas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:text="   Bebidas              "
            android:layout_gravity="right|top"
            android:onClick="onclick" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:layout_gravity="right|bottom">

            <Button
                android:id="@+id/buttonsobremesa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignTop="@+id/imageView2"
                android:layout_centerHorizontal="true"
                android:text="   Sobremesa       "
                android:layout_gravity="right|center_vertical"
                android:onClick="onclick" />
        </FrameLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="126dp"
        android:layout_height="59dp"
        android:layout_gravity="left|top">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Logout"
            android:id="@+id/buttonlogout"
            android:layout_gravity="center_horizontal|bottom"
            android:onClick="onclick" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="120dp"
        android:layout_height="127dp"
        android:layout_gravity="right|top">

        <ImageView
            android:layout_width="77dp"
            android:layout_height="77dp"
            android:id="@+id/imageView"
            android:src="@drawable/walltel1"
            android:layout_gravity="center_horizontal|top" />
    </FrameLayout>
    <TextView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/final_total"
        />

</FrameLayout>
